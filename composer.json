{
    "name": "eslym/laravel-easy-theme",
    "description": "Easy Theme System For Laravel",
    "type": "library",
    "version": "0.1beta",
    "require": {
        "php": ">=7.1",
        "laravel/framework": ">=5.0 <6.0"
    },
    "license": "mit",
    "authors": [
        {
            "name": "Eslym",
            "email": "engshun.lim@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Eslym\\EasyTheme\\": "src/"
        },
        "files": [
            "helpers/helper.php"
        ]
    },
    "extra": {
        "laravel":{
            "providers": [
                "Eslym\\EasyTheme\\Providers\\ThemeServiceProvider"
            ],
            "aliases": {
                "Theme": "Eslym\\EasyTheme\\Theme"
            }
        }
    }
}
